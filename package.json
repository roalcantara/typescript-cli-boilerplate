    "@types/cheerio": "^0.22.7",
    "@types/commander": "^2.12.2",
    "@types/faker": "^4.1.2",
    "@types/inquirer": "^0.0.38",
    "@types/node": "^9.6.0",
    "@types/ora": "^1.3.3",
    "faker": "^4.1.0",
    "rxjs-tslint-rules": "^3.15.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
