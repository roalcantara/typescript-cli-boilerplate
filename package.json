{
  "name": "typescript-cli-boilerplate",
  "version": "0.1.0",
  "description": "A Typescript CLI Boilerplate",
  "engines": {
    "node": "10",
    "yarn": ">=1.9.0 <2.0.0"
  },
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "tsc": "tsc",
    "lint": "tslint --project tsconfig.json src/**/*.ts",
    "test": "./node_modules/.bin/ts-mocha -p . src/**/**.spec.ts",
    "pokedex": "ts-node src/commands/pokedex.cmd.ts",
    "imdb": "ts-node src/commands/imdb.cmd.ts"
  },
  "keywords": [
    "typescript",
    "cli",
    "boilerplate"
  ],
  "author": "@roalcantara",
  "url": "https://github.com/roalcantara/typescript-cli-boilerplate/issues",
  "email": "rogerio.alcantara@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/cheerio": "^0.22.16",
    "@types/commander": "^2.12.2",
    "@types/faker": "^4.1.9",
    "@types/inquirer": "^6.5.0",
    "@types/node": "^13.7.0",
    "@types/ora": "^3.2.0",
    "faker": "^4.1.0",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "cheerio": "^1.0.0-rc.3",
    "commander": "^4.1.0",
    "inquirer": "^7.0.4",
    "ora": "^4.0.3",
    "pokedex-promise-v2": "^3.2.0",
    "puppeteer": "^2.1.0",
    "rxjs": "^6.5.4"
  }
}
