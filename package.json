{
  "name": "typescript-cli-boilerplate",
  "version": "0.1.0",
  "description": "A Typescript CLI Boilerplate",
  "engines": {
    "node": "10",
    "yarn": ">=1.9.0 <2.0.0"
  },
  "main": "ts-node src/main.ts",
  "scripts": {
    "setup": "yarn global add typescript && prettier && npx && jest",
    "clear": "rm -r ./dist && rm -r .tmp || true",
    "purge": "yarn clear && rm -rf node_modules",
    "reset": "yarn purge && yarn",
    "build": "npx tsc --p tsconfig.json",
    "tsc": "npx tsc",
    "lint": "npx tslint --project tsconfig.json src/**/*.ts",
    "test": "npx jest",
    "start": "ts-node src/lib/main.ts"
  },
  "keywords": [
    "typescript",
    "cli",
    "boilerplate"
  ],
  "author": "@roalcantara",
  "url": "https://github.com/roalcantara/typescript-cli-boilerplate/issues",
  "email": "rogerio.alcantara@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/cheerio": "^0.22.16",
    "@types/faker": "^4.1.9",
    "@types/inquirer": "^6.5.0",
    "@types/lodash": "^4.14.149",
    "@types/jest": "^25.1.1",
    "@types/node": "^13.7.0",
    "@types/ora": "^3.2.0",
    "factory-bot-ts": "^0.1.5",
    "faker": "^4.1.0",
    "html-loader-jest": "^0.2.1",
    "jest": "^25.1.0",
    "ts-jest": "^25.1.0",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "cheerio": "^1.0.0-rc.3",
    "inquirer": "^7.0.4",
    "lodash": "^4.17.15",
    "ora": "^4.0.3",
    "pokedex-promise-v2": "^3.2.0",
    "puppeteer": "^2.1.0",
    "rxjs": "^6.5.4",
    "yargs": "^15.1.0"
  }
}
