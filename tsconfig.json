{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es2017","es2015","dom","es6"
    ],
    "module": "commonjs",
    "outDir": "./dist",
    "sourceMap": false,
    "strict": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "strictNullChecks": false
  },
  "include": [
    "./**/**.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
